<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.programmingproject.gui.controllers.GameWonController">
   <children>
      <Label layoutX="230.0" layoutY="62.0" text="YOU WON!">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="245.0" layoutY="424.0" mnemonicParsing="false" onMouseClicked="#switchToMenu" prefHeight="60.0" prefWidth="160.0" style="-fx-border-color: grey;" styleClass="back-button" stylesheets="@lightmode.css" text="Back to menu">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label fx:id="timeLabel" layoutX="82.0" layoutY="333.0" prefHeight="26.0" prefWidth="485.0" style="-fx-border-color: grey; -fx-border-radius: 20; -fx-alignment: center; -fx-font-weight: bold;" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font></Label>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="250.0" layoutY="140.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@win.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
